{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "save": "devDependencies",
      "description": "Add and bootstrap ngx-cli-toolkit",
      "factory": "./ng-add/index#ngAdd"
    },
    "init": {
      "factory": "./ng-generate/init/index#initSchematicsProject",
      "description": "Initialize schematics project",
      "schema": "./ng-generate/init/schema.json"
    },
    "add-schematic": {
      "factory": "./ng-generate/add-schematic/index#createAddSchematic",
      "description": "Create ng-add schematic",
      "schema": "./ng-generate/add-schematic/schema.json"
    },
    "generate-schematic": {
      "factory": "./ng-generate/generate-schematic/index#createGenerateSchematic",
      "description": "Create ng-generate schematic",
      "schema": "./ng-generate/generate-schematic/schema.json"
    }
  }
}
